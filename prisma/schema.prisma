datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") // Configurez votre URL dans .env
}

generator client {
  provider = "prisma-client-js"
}

model Inscription {
  id             Int      @id @default(autoincrement())
  nom            String
  prenom         String
  adresse        String
  codePostal     String
  ville          String
  telephone      String
  email          String
  stage          String
  nationalite    String
  dateNaissance  DateTime
  pieceDId       String  
  permis         String
  createdAt      DateTime @default(now())
}
